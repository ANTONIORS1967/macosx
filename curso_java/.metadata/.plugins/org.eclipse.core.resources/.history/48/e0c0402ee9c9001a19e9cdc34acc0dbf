package classe;


public class DataTeste {

	public static void main(String[] args) {

		Data d1 = new Data();
		d1.dia = 15;
		d1.mes = 7;
		d1.ano = 2020;
		
		var d2 = new Data();
		d2.dia= 8;
		d2.mes = 11;
		d2.ano = 2021; 
		
		
		System.out.println(d1.obterDataFormatada());
		System.out.println(d2.obterDataFormatada());
	}

}
